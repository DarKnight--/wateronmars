{% extends "wom_river/base.html_dt" %}

{% block title %}
{{ title_qualify }} Sources
{% endblock %}

{% block content %}
<p><i class="icon-book"></i><strong>{{ title_qualify }} sources</strong></p>
{% if source_list %}
    <ul>
      {% for source in source_list %}
      <li><a href="{{ source.url }}">{{ source.name }}</a> <span class="label" title="number of pebbles">{{ source.reference_set.count }}</span></li>
      {% endfor %}
    </ul>
{% endif %}
{% endblock %}
